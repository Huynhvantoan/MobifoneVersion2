<?xml version="1.0" encoding="utf-8"?>
<com.toan_itc.mobifone.libs.view.StateLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/stateLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="16dp"
      android:layout_marginRight="16dp"
      android:layout_marginTop="100dp"
      android:orientation="vertical"
      >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/c_ng_ty_tnhh_tmdv_vi_n_th_ng_th_nh_nh_n"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp"
        />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginRight="70dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        >
      <TextView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:text="@string/email"
          android:textColor="@color/md_black_1000"
          android:textSize="18sp"
          />
      <TextView
          android:id="@+id/txt_email"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:text="@string/vienthongthanhnhan_gmail_com"
          android:textColor="@color/emailbackground"
          android:textSize="17sp"
          />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        >
      <TextView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:text="@string/di_ng"
          android:textColor="@color/md_black_1000"
          android:textSize="18sp"
          />
      <TextView
          android:id="@+id/txt_phone"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:text="@string/_0901067979"
          android:textColor="@color/emailbackground"
          android:textSize="18sp"
          />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        >
      <TextView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:text="@string/hotline"
          android:textColor="@color/md_black_1000"
          android:textSize="18sp"
          />
      <TextView
          android:id="@+id/txt_hotline"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:text="@string/_0903669889"
          android:textColor="@color/emailbackground"
          android:textSize="18sp"
          />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        >
      <TextView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:text="@string/website"
          android:textColor="@color/md_black_1000"
          android:textSize="18sp"
          />
      <TextView
          android:id="@+id/txt_website"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="5dp"
          android:lines="2"
          android:text="@string/http_vienthongthanhnhan_com"
          android:textColor="@color/emailbackground"
          android:textSize="18sp"
          />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        >
      <TextView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:text="@string/a_ch"
          android:textColor="@color/md_black_1000"
          android:textSize="18sp"
          />
      <TextView
          android:id="@+id/txt_address"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:text="@string/_588_l_c_th_p15_q_g_v_p_tp_hcm"
          android:textColor="@color/md_black_1000"
          android:textSize="16sp"
          />
    </LinearLayout>

  </LinearLayout>
</com.toan_itc.mobifone.libs.view.StateLayout>
